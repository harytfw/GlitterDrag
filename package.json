{
    "name": "glitterdrag",
    "description": "An drag extension for Firefox Browser.",
    "devDependencies": {
        "eslint": "^4.4.1",
        "web-ext": "^2.9.1"
    },
    "scripts": {
        "build": "npm run renew && web-ext build -s ./src --overwrite-dest",
